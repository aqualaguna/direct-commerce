{
  "name": "strapi",
  "version": "0.1.0",
  "private": true,
  "description": "A Strapi application",
  "scripts": {
    "build": "strapi build",
    "console": "strapi console",
    "deploy": "strapi deploy",
    "dev": "strapi develop",
    "develop": "strapi develop",
    "start": "strapi start",
    "strapi": "strapi",
    "test": "jest --forceExit --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand --no-cache",
    "test:debug:watch": "node --inspect node_modules/.bin/jest --runInBand --watch",
    "test:integration": "node scripts/run-integration-tests.js",
    "test:integration:watch": "node scripts/run-integration-tests.js watch",
    "test:integration:coverage": "node scripts/run-integration-tests.js coverage --keep-server",
    "test:integration:ci": "node scripts/run-integration-tests.js run",
    "test:integration:setup": "node scripts/test-server.js ensure",
    "test:integration:start": "node scripts/test-server.js start",
    "test:integration:stop": "node scripts/test-server.js stop",
    "test:integration:status": "node scripts/test-server.js status",
    "test:integration:health": "node scripts/test-server.js health",
    "setup:env": "node scripts/setup-env.js dev",
    "setup:env:prod": "node scripts/setup-env.js production",
    "validate:env": "node scripts/setup-env.js validate",
    "env:info": "node scripts/setup-env.js info",
    "quality": "npm run type-check && npm run lint && npm run format:check && npm run test:ci",
    "quality:fast": "npm run type-check && npm run lint && npm run format:check",
    "quality:fix": "npm run lint:fix && npm run format && npm run test:ci",
    "lint": "eslint src --ext .ts,.js",
    "lint:fix": "eslint src --ext .ts,.js --fix",
    "lint:api": "eslint src/api --ext .ts,.js",
    "lint:api:fix": "eslint src/api --ext .ts,.js --fix",
    "format": "prettier --write src/**/*.{ts,js,json}",
    "format:check": "prettier --check src/**/*.{ts,js,json}",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "clean": "rm -rf dist build .strapi coverage",
    "clean:node_modules": "rm -rf node_modules package-lock.json",
    "clean:all": "npm run clean && npm run clean:node_modules",
    "reinstall": "npm run clean:node_modules && npm install",
    "pre-commit": "npm run quality:fast",
    "pre-push": "npm run quality",
    "upgrade": "npx @strapi/upgrade latest",
    "upgrade:dry": "npx @strapi/upgrade latest --dry"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "@strapi/plugin-cloud": "5.23.1",
    "@strapi/plugin-documentation": "^5.23.3",
    "@strapi/plugin-users-permissions": "^5.23.1",
    "@strapi/provider-email-sendgrid": "^5.23.1",
    "@strapi/provider-upload-aws-s3": "^5.23.1",
    "@strapi/strapi": "5.23.1",
    "better-sqlite3": "11.3.0",
    "cropperjs": "^2.0.1",
    "express-rate-limit": "^8.0.1",
    "geoip-lite": "^1.4.10",
    "node-cron": "^4.2.1",
    "pg": "^8.16.3",
    "prismjs": "^1.30.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.0.0",
    "strapi-plugin-schema-visualizer": "^1.0.5",
    "styled-components": "^6.1.19",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/pg": "^8.15.5",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "dotenv": "^17.2.2",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.1.1",
    "prettier": "^3.3.3",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0 <=22.x.x",
    "npm": ">=6.0.0"
  },
  "strapi": {
    "uuid": "c4e3bd6c-1475-4692-b95c-c090fbf7507f",
    "installId": "08b3103d7cf661dbde88496aa39988a4b285a9165afe9d7a4b2e53a31d1aa10b"
  }
}
